{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "BloodSugerSensor Remote Debug (Node.js)",
      "type": "node",
      "request": "attach",
      "port": 9229,
      "address": "localhost",
      "localRoot": "${workspaceRoot}/modules/BloodSugerSensor",
      "remoteRoot": "/app",
      "protocol": "inspector"
    },
    {
      "name": "BloodSugerSensor Local Debug (Node.js)",
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}/modules/BloodSugerSensor/app.js",
      "console": "integratedTerminal",
      "env": {
        "EdgeHubConnectionString": "${config:azure-iot-edge.EdgeHubConnectionString}",
        "EdgeModuleCACertificateFile": "${config:azure-iot-edge.EdgeModuleCACertificateFile}"
      }
    },
